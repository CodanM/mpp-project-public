<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net"/>
    </configSections>
    <log4net>
        <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="%d{DATE} %-5level %class{1} %L %M - %m%n"/>
            </layout>
        </appender>
        <root>
            <level value="ALL"/>
            <appender-ref ref="ConsoleAppender"/>
        </root>
    </log4net>
    <connectionStrings>
        <add name="contest-mgmt-db" connectionString="Data Source=C:/Users/danie/Desktop/MocsiDanielAndrei225-1/mpp-proiect-repository-CodanM/data/contest-management-db"/>
    </connectionStrings>
</configuration>